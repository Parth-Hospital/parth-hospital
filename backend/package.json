{
  "name": "parth-hospital-backend",
  "version": "1.0.0",
  "description": "Parth Hospital Healthcare ERP Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "npx prisma generate && tsc && npx tsc-alias",
    "start": "node dist/server.js",
    "postinstall": "npx prisma generate",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "npx prisma db push",
    "prisma:seed": "tsx prisma/seed.ts",
    "cleanup": "tsx scripts/cleanup.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "healthcare",
    "erp",
    "hospital-management"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.0.0",
    "@fastify/rate-limit": "^9.1.0",
    "@prisma/client": "5.19.0",
    "@types/dompurify": "3.0.5",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.0",
    "dompurify": "3.3.1",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.0",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "2.35.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^4.0.0",
    "multer": "^1.4.5-lts.1",
    "prisma": "5.19.0",
    "sharp": "0.34.5",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "validator": "13.15.26",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.9",
    "@types/validator": "13.15.10",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.2"
  },
  "resolutions": {
    "prisma": "5.19.0",
    "@prisma/client": "5.19.0"
  }
}
